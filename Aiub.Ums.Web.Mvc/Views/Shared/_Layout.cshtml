<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>UMS | @ViewBag.PageHeader</title>
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    @Styles.Render("~/UmsBundledStyles")
    <!--[if lt IE 9]>
    <script src="/Scripts/html5shiv.min.js"></script>
    <script src="/Scripts/respond.min.js"></script>
    <![endif]-->
</head>
<body class="hold-transition skin-blue layout-top-nav">
    <div class="wrapper" style="height: auto; min-height: 100%;">
        <header class="main-header">
            <nav class="navbar navbar-static-top">
                <div class="container">
                    <div class="navbar-header">
                        <a href="../../index2.html" class="navbar-brand"><b>AIUB</b> UMS</a>
                        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar-collapse">
                            <i class="fa fa-bars"></i>
                        </button>
                    </div>
                    <!-- Collect the nav links, forms, and other content for toggling -->
                    <div class="collapse navbar-collapse pull-left" id="navbar-collapse">
                        <ul class="nav navbar-nav">
                            <li class="active"><a href="/Home/Index">Home</a></li>
                            <li><a href="/Home/About">About Us <span class="sr-only">(current)</span></a></li>
                            <li><a href="/Account/Login">Login</a></li>
                        </ul>
                        <form class="navbar-form navbar-left" role="search">
                            <div class="form-group">
                                <input class="form-control" id="navbar-search-input" placeholder="Search" type="text">
                            </div>
                        </form>
                    </div>
                </div>
            </nav>
        </header>
        <!-- Full Width Column -->
        <div class="content-wrapper" style="min-height: 144px;">
            <div class="container">
                <section class="content-header">
                    <h1>
                        @ViewBag.PageHeader
                        <small>@ViewBag.PageDescription</small>
                    </h1>
                    <ol class="breadcrumb">
                        <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
                        <li><a href="#">Layout</a></li>
                        <li class="active">Top Navigation</li>
                    </ol>
                </section>
                <!-- Main content -->
                <section class="content">
                    @RenderBody()
                </section>
            </div>
        </div>
        <footer class="main-footer">
            <div class="container">
                <div class="pull-right hidden-xs">
                    A demo project for<b>ATP-II</b>
                </div>
                <strong>Copyright © 2018 <a href="https://adminlte.io">Aiub</a>.</strong> All rights reserved.
            </div>
            <!-- /.container -->
        </footer>
    </div>
@Scripts.Render("~/UmsBundledScripts")
    @RenderSection("UmsScripts")
</body>
</html>